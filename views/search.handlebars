<div id="body">
    <div class= "container">
        
        <div>
            {{#each articles}}
                <div id="article-card" class="card col-3">
                    {{#if photoPath}}<img class="card-img-top" src="/assets/photos/{{photoPath}}" alt="Card image cap" style="width: 50; height: 50;">{{/if}}
                    <div class="card-body">
                        <h5 class="card-title" style="color:black">Florida Man {{headline}}</h5>
                        <a href="/api/article/{{id}}" class="btn btn-danger">View Article</a>
                    </div>
                </div>
            {{/each}}
        </div> 
    </div>
</div>


<footer class="site-footer">
    </div>
    <p>Copyright &copy</p>
    </div>
</footer>
